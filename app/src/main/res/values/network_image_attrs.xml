<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="url" format="string" />
    <attr name="preview" format="string" />
    <declare-styleable name="NetworkImageView">
        <attr name="url" />
        <attr name="preview" />
    </declare-styleable>
    <declare-styleable name="CircleImageView" parent="NetworkImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
</resources>